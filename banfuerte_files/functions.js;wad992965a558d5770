      //HABILITACION DE REDES SOCIALES EN EL FOOTER
      function getSocialDomain(aHrefUrl, dominio){
          //TAGGEO AUTOMATICO DE PAGINAS INTERNAS
          var redSocial = aHrefUrl.split('.com')[0].split('www.')[1];
          if(getUrlDomain() == "www.banorte"){    
            gaTagCreate('send', 'event', dominio.toLowerCase().replace(/\b[a-z]/g, function(letter){return letter.toUpperCase()}) + ' - Redes Sociales', 'Redirigir', redSocial.toLowerCase().replace(/\b[a-z]/g, function(letter){return letter.toUpperCase()}) + ' - Footer');
          }
      }
      if(window.location.href.indexOf('/wps/portal/gfb')>0 || window.location.href.indexOf('/wps/myportal/gfb')>0 || window.location.href.indexOf('/wps/portal/banorte')>0 || window.location.href.indexOf('/wps/myportal/banorte')>0 || window.location.href.indexOf('/wps/portal/ixe/')>0 || window.location.href.indexOf('/wps/myportal/ixe/')>0  || dominioAudiencias2 == null || dominioAudiencias2 == undefined){
        $(".redes-sociales").show();
      }
      $(".redes-sociales a").on("click", function(){
        if(window.location.href.indexOf('/wps/portal/gfb')>0 || window.location.href.indexOf('/wps/myportal/gfb')>0){
            getSocialDomain($(this).attr("href"), "gfb");
        }else if(window.location.href.indexOf('/wps/portal/banorte')>0 || window.location.href.indexOf('/wps/myportal/banorte')>0 || dominioAudiencias2 == null || dominioAudiencias2 == undefined){
            getSocialDomain($(this).attr("href"), "banorte");
        }else if(window.location.href.indexOf('/wps/portal/ixe/')>0 || window.location.href.indexOf('/wps/myportal/ixe/')>0){
            getSocialDomain($(this).attr("href"), "ixe");
        }else if(window.location.href.indexOf('/wps/portal/ixe-xima')>0 || window.location.href.indexOf('/wps/myportal/ixe-xima')>0){
            getSocialDomain($(this).attr("href"), "ixe-xima");
        }else if(window.location.href.indexOf('/wps/portal/empresas')>0 || window.location.href.indexOf('/wps/myportal/empresas')>0){
            getSocialDomain($(this).attr("href"), "empresas");
        }
      });
      //FIN HABILITACION DE REDES SOCIALES EN EL FOOTER
    });
  })();
