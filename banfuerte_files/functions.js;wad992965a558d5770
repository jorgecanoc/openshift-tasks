      function changeFooter(c, r){
        //c: cambiar, r: remover
        try{
          var footerEle = $('.footer_sitemap .layout  ul  li');
          $(footerEle[c]).html('<div class="d_st"><a href="/wps/portal/gfb">Grupo Financiero Banorte</a></div>');
          if(r)
            $(footerEle[r]).remove();
        }catch(e){
          console.log(e);
        }
      }
